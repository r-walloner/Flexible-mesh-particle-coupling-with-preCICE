FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      ExplicitMomentum;
}

dimensions      [1 0 0 0 0 0 0];
internalField   uniform (0 0 0);
boundaryField
{
    "bottom|spout|top|sides"
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    "front|back"
    {
        type            cyclic;
    }
}
